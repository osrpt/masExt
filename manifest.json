{
    "manifest_version": 2,
    "name": "masExt",
    "version": "1.0.0",
    "description": "massimo clean web extension",
    "permissions": [
        "contextMenus",
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "/icons/icon-16.png",
        "24": "/icons/icon-24.png",
        "48": "/icons/icon-48.png",
        "128": "/icons/icon-128.png"
    },
    "minimum_chrome_version": "6.0.0.0",
    "browser_action": {
        "default_icon": "/icons/icon-48.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://t66y.com/*",
            "http://www.rmdown.com/*",
            "http://www.zhibo8.cc/*",
            "http://www.cnblogs.com/*",
            "http://weibo.com/*",
            "http://user.qzone.qq.com/*"
        ],
        "css": ["style.css"],
        "js": ["jquery-2.0.3.min.js", "contentScripts.js", "inotev2.js", "jquery.lazyload.min.js"]
    }]
}
